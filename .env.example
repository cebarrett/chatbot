# OpenAI Configuration
# Copy this file to .env and fill in your values

# Required: Your OpenAI API key
VITE_OPENAI_API_KEY=your-api-key-here

# Optional: OpenAI model (defaults to gpt-4o-mini)
VITE_OPENAI_MODEL=gpt-4o-mini

# S3 Deployment Configuration

# Required for deployment: Your S3 bucket name
S3_BUCKET=your-bucket-name

# Optional: AWS region (defaults to us-east-1)
AWS_REGION=us-east-1

# Optional: CloudFront distribution ID for cache invalidation
CLOUDFRONT_DISTRIBUTION_ID=
