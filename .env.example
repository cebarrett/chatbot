# AppSync Configuration
# Copy this file to .env and fill in your values after running `terraform apply`

# Required: AppSync GraphQL endpoint URL
# Get this from Terraform output: terraform output -raw appsync_api_url
VITE_APPSYNC_URL=https://your-appsync-id.appsync-api.us-east-1.amazonaws.com/graphql

# Required: AppSync API Key
# Get this from Terraform output: terraform output -raw appsync_api_key
VITE_APPSYNC_API_KEY=your-appsync-api-key

# Optional: AWS region (defaults to us-east-1)
VITE_AWS_REGION=us-east-1

# S3 Deployment Configuration

# Required for deployment: Your S3 bucket name
S3_BUCKET=your-bucket-name

# Optional: AWS region (defaults to us-east-1)
AWS_REGION=us-east-1

# Optional: CloudFront distribution ID for cache invalidation
CLOUDFRONT_DISTRIBUTION_ID=
